{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.6.1",
		"silverstripe/framework": "3.7.7",
		"silverstripe/reports": "3.6.1",
		"silverstripe/siteconfig": "3.6.1",
		"silverstripe-themes/simple": "3.1.*",
		"unclecheese/display-logic": "^1.5",
		"symbiote/silverstripe-gridfieldextensions": "1.4.x-dev",
		"sheadawson/silverstripe-linkable": "^1.3",
		"arillo/gridfieldrelationhandler": "dev-master",
		"undefinedoffset/sortablegridfield": "^0.6.10",
		"stevie-mayhew/hasoneedit": "^1.0",
		"silverstripe/spamprotection": "2.*",
		"undefinedoffset/silverstripe-nocaptcha": "1.*"
	},
	"require-dev": {
		"phpunit/phpunit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.6.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
